a = [1,14.123123,5,12.1115,1,5,15,1,5.123123,15,1,5,15]
b = [1,14,5.123123,1.14125,1,5,15,1.123,5,15,1,5,15]
c  = [1,14.123123,5,125,1,5.123,15,1,5.123123,15,1,5,15]
i = 0
print('{:.>2}'.format('a[i]'),'{:.>2}'.format('b[i]'),'{:.>2}'.format('c[i]'))
while (i <10):
    i += 1  
    print('{:.>2.2%}'.format(a[i]), '{:.>2.2%}'.format(b[i]), '{:.>2.2%}'.format(c[i]))
